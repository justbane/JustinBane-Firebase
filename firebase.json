{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run"
    ]
  },
  "hosting": {
    "public": "public",
    "rewrites": [
        {
            "source": "**",
            "destination": "/index.html"
        }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
